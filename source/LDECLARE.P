@l_the:	PUSH1;
	 CAR; SET2_1;			/指定された型
	POP1;
	SPUSH2;
	 CDR; CAR; !l_eval;		/本体の評価
	SPOP2;
	PUSH1;
	 !l_typep; NILP;		/評価結果が指定された型と同じか
	POP1; je TYPE_ERR;		/違っていたらエラー
	ret;				/同じならばそのまま返す

